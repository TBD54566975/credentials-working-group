{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Identity Verification Evidence",
  "description": "Evidence for an identity verification result",
  "type": "object",
  "properties": {
    "method": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "match": {
      "type": "object",
      "properties": {
        "result": {
          "type": "string"
        },
        "unmatchedData": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "verifierName": {
      "type": "string"
    },
    "documentType": {
      "type": "string"
    },
    "issuingStateOrProvince": {
      "type": "string"
    },
    "issuingCountry": {
      "type": "string"
    },
    "idNumber": {
      "type": "string"
    },
    "expiryDate": {
      "type": "string"
    }
  },
  "additionalProperties": false
}